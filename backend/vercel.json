{
    "version": 2,
    "builds": [
      {
        "src": "frontend/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ]
  }